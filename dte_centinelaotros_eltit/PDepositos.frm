    Private Sub Form_Activate()
        Principal.barraEstado.Panels(1).text = UCase(Me.Caption)
    End Sub
    
    Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
        If KeyCode = 27 Then
            Unload Me
        End If
        If KeyCode = 38 Then
            If Screen.ActiveForm.ActiveControl.Name = "dato1" Then
                Unload Me
            End If
        End If
    End Sub
    
    Private Sub Form_Load()
        modifica = False
        Call Centrar(Me)
    End Sub
    
    Private Sub Form_Unload(Cancel As Integer)
        Principal.barraEstado.Panels(1).text = UCase(Principal.Caption)
        Call limpiaBarra(2)
    End Sub

'=============================================================================
'PASA LOS DATOS DE LOS CONTROLES A LA ESTRUCTURA
'=============================================================================
    Private Sub ctrltostruct()
        D.loc = empresaActiva
        D.numero = dato1.text
        D.fecha = dato4.text & "-" & dato3.text & "-" & dato2.text
        D.Banco = dato5.text
        D.MONTO = dato6.text
        D.tipo = dato7.text
        D.rut = dato8.text & lblDv.Caption
        Call grabarDeposito(D, modifica)
        Call retorno
    End Sub
'=============================================================================
'PASA LOS DATOS DE LOS CONTROLES A LA ESTRUCTURA
'=============================================================================

'=============================================================================
'PASA LOS DATOS DE LA ESTRUCTURA A LOS CONTROLES
'=============================================================================
    Private Sub structtoctrl()
        dato1.text = D.numero
        dato2.text = Format(D.fecha, "dd")
        dato3.text = Format(D.fecha, "mm")
        dato4.text = Format(D.fecha, "yyyy")
        dato5.text = D.Banco
        dato6.text = D.MONTO
        dato7.text = D.tipo
        dato8.text = D.rut
        Call dato5_KeyPress(13)
        Call dato7_KeyPress(13)
        lblDv.Caption = rut(dato8.text)
        lblNombre.Caption = leerNombreCliente(D.rut)
        Call DeshabilitarCajas(Me)
    End Sub
